    "style-loader": "^0.18.1",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.20.0",
    "classlist-polyfill": "^1.0.3",
    "closest": "0.0.1",
    "dom-delegate": "^2.0.3",
    "feature.js": "^1.0.0",
    "font-awesome": "^4.4.0",
    "lodash.debounce": "^4.0.8",
    "mustache": "^2.2.1",
