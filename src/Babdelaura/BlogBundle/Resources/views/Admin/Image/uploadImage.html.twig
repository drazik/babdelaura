<div class="js-image-upload">
    <form class="js-image-upload-form" method="post" action="{{ path('babdelaurablog_admin_uploaderImage') }}" {{ form_enctype(form) }}>
        <div class="form-group">
            {{ form_label(form.file, "Image") }}
            {{ form_errors(form.file)}}
            {{ form_widget(form.file) }}
        </div>
        <div class="form-group">
            {{ form_label(form.watermark) }}
            {{ form_errors(form.watermark)}}
            {{ form_widget(form.watermark) }}
        </div>
        {{ form_rest(form) }}

        <button class="bab-Button" type="submit">Ajouter l'image</button>
    </form>
</div>
