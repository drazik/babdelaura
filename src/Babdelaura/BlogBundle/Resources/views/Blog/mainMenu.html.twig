<button class="bab-MainNav-toggle js-nav-toggle">
    <i class="fa fa-bars js-nav-toggle-icon"></i>
</button>
<ul class="bab-MainNav-container js-nav-items">
    <li class="bab-MainNav-item"><a href="{{ path('babdelaurablog_accueil') }}" title="">Accueil</a></li>

    {% for categorie in listeCategories %}
        <li class="bab-MainNav-item">
            <a href="{{ path('babdelaurablog_categorie', {'slug' : categorie.slug}) }}" title="{{categorie.nom}}">
                {{categorie.nom}}
            </a>
        </li>
    {% endfor %}

    {% for page in listePages %}
        <li class="bab-MainNav-item">
            <a href="{{ path('babdelaurablog_page', {'slug' : page.slug}) }}" title="{{page.titre}}">{{page.titre}}</a>
        </li>
    {% endfor %}
    <li class="bab-MainNav-item"><a href="{{ path('babdelaurablog_contact') }}" title="">Contact</a></li>
</ul>
